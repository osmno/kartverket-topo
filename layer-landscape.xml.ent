
<Style name="leisure">
  <Rule>
    <Filter>[landuse] = 'brownfield' or [landuse] = 'landfill' or [landuse] = 'greenfield' or [landuse] = 'construction' or [landuse] = 'quarry'</Filter>
    &maxscale_zoom10;
    <PolygonSymbolizer>
      <CssParameter name="fill">&regulatedarea-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[natural] = 'wood' or [landuse] = 'wood' or [landuse] = 'forest'</Filter>
    &maxscale_zoom8;
    <PolygonSymbolizer>
      <CssParameter name="fill">&forestland-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom12;
    <Filter>([landuse] = 'cemetery' or [landuse] = 'grave_yard' or [amenity] = 'grave_yard')</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&openland-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[leisure] = 'common' or [leisure] = 'park' or [leisure] = 'recreation_ground' or [leisure] = 'golf_course' or [landuse] = 'sport' or [landuse] = 'recreational' or [leisure] = 'winter_sport'</Filter>
    &maxscale_zoom11;
    <PolygonSymbolizer>
      <CssParameter name="fill">&openland-color;</CssParameter>
   </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[landuse]='playground' or [leisure]='playground'</Filter>
    &maxscale_zoom12;
    <PolygonSymbolizer>
      <CssParameter name="fill">&openland-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[landuse] = 'park' or [landuse] = 'recreation_ground' or [landuse] = 'village_green' or [landuse] = 'green'</Filter>
    &maxscale_zoom11;
    <PolygonSymbolizer>
      <CssParameter name="fill">&openland-color;</CssParameter>
   </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom11;
    <Filter>[tourism] = 'camp_site' or [tourism]='caravan_site' or [tourism] = 'picnic_site'</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&openland-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom11;
    <Filter>[natural] = 'field' or [natural] = 'heath' or [landuse] = 'conservation'</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&background-color;</CssParameter>
     </PolygonSymbolizer>
  </Rule>
 <Rule>
    <Filter>[natural] = 'scrub'</Filter>
    &maxscale_zoom12;
    <PolygonSymbolizer>
      <CssParameter name="fill">&background-color;</CssParameter>
     </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom8;
     <Filter>[landuse] = 'residential' or [landuse] ='commercial' or [landuse] = 'retail'</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&urbanarea-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom8;
     <Filter>[landuse] = 'industrial' or [power]='station' or [power]='sub_station'</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&regulatedarea-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom12;
     <Filter>[landuse] = 'school' or [amenity] = 'school' or [amenity] = 'college' or [amenity] = 'university' or [amenity]='kindergarten' or [military] = 'barracks' or [amenity] = 'hospital'</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&urbanarea-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[leisure] = 'garden' or [landuse] = 'allotments' or [landuse] = 'military' or [landuse] = 'railway' or [landuse] = 'garden' or [tourism] = 'zoo'</Filter>
    &maxscale_zoom12;
    <PolygonSymbolizer>
      <CssParameter name="fill">&urbanarea-color;</CssParameter>
   </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[landuse] = 'field' or [natural] = 'crop' or [landuse] = 'farm' or [landuse] = 'farmland' or [landuse] = 'meadow' or [landuse] = 'grass' or [natural] = 'grass' or [natural] = 'grassland' or [natural] = 'meadow'</Filter>
    &maxscale_zoom11;
    <PolygonSymbolizer>
      <CssParameter name="fill">&farmland-color;</CssParameter>
   </PolygonSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom11;
    <Filter>[landuse] = 'orchard' or [landuse] = 'vineyard'</Filter>
    <PolygonSymbolizer>
      <CssParameter name="fill">&farmland-color;</CssParameter>
     </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[natural] = 'mud' or [natural] = 'beach' or [natural] = 'sand'</Filter>
    &maxscale_zoom12;
    <PolygonSymbolizer>
       <CssParameter name="fill">&background-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Filter>[leisure] = 'track'</Filter>
    &maxscale_zoom12;
     <LineSymbolizer>
      <CssParameter name="stroke">&black;</CssParameter>
      <CssParameter name="stroke-linejoin">round</CssParameter>
      <CssParameter name="stroke-linecap">round</CssParameter>
      <CssParameter name="stroke-width">0.75</CssParameter>
      <CssParameter name="stroke-dasharray">4,2</CssParameter>
    </LineSymbolizer>
  </Rule>
</Style>

<Style name="sports_grounds">
  <Rule>
    &maxscale_zoom12;
   <PolygonSymbolizer>
       <CssParameter name="fill">&openland-color;</CssParameter>
    </PolygonSymbolizer>
  </Rule>
</Style>

<Style name="islands">
  <Rule>
    &maxscale_zoom8;
    &minscale_zoom11;
