      <CssParameter name="stroke-linejoin">round</CssParameter>
      <CssParameter name="stroke-linecap">round</CssParameter>
    </LineSymbolizer>
  </Rule>
  <Rule>
    &maxscale_zoom13;
    <PolygonSymbolizer>
      <CssParameter name="fill">&background-color;</CssParameter>
    </PolygonSymbolizer>
